<h2>edit</h2>

<%=form_for [@house, @character] do |f|  %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :img_url %>
  <%= f.text_field :img_url %>
  <%= f.submit "Update Character" %>
<% end %>

<%=form_for [@house, @character], method: :delete do |f| %>
  <%= f.submit "Delete", class: "delete" %>
 <% end %>
