<h2>New House</h2>

<%= form_for @house do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :motto %>
  <%= f.text_field :motto %>
  <%= f.label :sigil %>
  <%= f.text_field :sigil %>
  <%= f.label :img_url %>
  <%= f.text_field :img_url %>
  <%= f.submit "Add House" %>
<% end %>
