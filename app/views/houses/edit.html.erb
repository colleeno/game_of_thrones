<%= form_for @house do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :motto %>
  <%= f.text_field :motto %>
  <%= f.label :sigil %>
  <%= f.text_field :sigil %>
  <%= f.label :img_url %>
  <%= f.text_field :img_url %>
<%= f.submit "Update House" %>
<% end %>

<%= form_for @house, method: :delete do |f| %>
  <%= f.submit "Delete House", class: "delete" %>
<% end %>
